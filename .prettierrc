{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "racketSameLine": false,
  "arrowParens": "always",
  "experimentalOperatorPosition": "start",
  "experimentalTernaries": true,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
